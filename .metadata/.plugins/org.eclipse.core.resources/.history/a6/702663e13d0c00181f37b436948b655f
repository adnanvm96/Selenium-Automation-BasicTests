package classesPractice;

public class ClassDemo {

	public static void main(String[] args) {
		
		Car bmw = new Car();
		bmw.setMake("BMW");
		System.out.println(bmw.getMake());
		
		Car benz = new Car();
		benz.setMake("C300");
		System.out.println(benz.getMake());
		
		benz.year = 2020;
		System.out.println(benz.year);
			
		
		
	}

}
